{
  "name": "project-viewer2",
  "main": "./src/index",
  "version": "0.2.30",
  "description": "Project viewer/manager that lets you add and remove groups/projects as well as switching between them.",
  "keywords": [
    "project",
    "manager",
    "viewer",
    "group"
  ],
  "repository": "https://github.com/jccguimaraes/atom-project-viewer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.5.3 <2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "latest",
    "cz-conventional-changelog": "latest",
    "eslint": "latest",
    "istanbul": "latest",
    "jscs": "latest",
    "loophole": "latest",
    "mocha": "latest",
    "sinon": "latest",
    "sinon-chai": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "atomTestRunner": "./custom/specs.js",
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "project-viewer-on": {
      "description": "Listens to a change in the selected group and/or project",
      "versions": {
        "0.3.0": "projectViewerOn"
      }
    },
    "project-viewer-emit": {
      "description": "Informs an intent to change the group and/or project",
      "versions": {
        "0.3.0": "projectViewerEmit"
      }
    }
  }
}
